<% obj.each do |obj| %>

  <div class="card">
    <input id="read" class="lower-btn" type="checkbox"/>
    <label for="read" class="lower"></label>

      <div class="card-head">
        <%= image_tag obj.get_image %>
      </div>

      <div class="card-body">
      <h4 class="card-title"><%= link_to obj.title, post_hobby_path(obj) %></h4>
      <div class="card-text">
        <p>
        <%= obj.text %>
        <%= obj.tags.map(&:name).join(',') %>
        <%= obj.post_status_i18n %>
        </p>
      </div>
    </div>
  </div>
<% end %>